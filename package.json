{
  "name": "dynamo-search",
  "version": "0.0.0",
  "description": "Inverted index document search for DynamoDB.",
  "license": "Apache-2.0",
  "author": "Alexander Van de kleut <avandekleut@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "clean": "rimraf coverage dist tmp && tsc --build --clean",
    "dynamodb-local": "docker compose up",
    "lint": "eslint . --ext .ts --ext .mts",
    "prepare": "husky install",
    "prettier": "prettier --config .prettierrc --ignore-path .gitignore --write .",
    "start": "node dist/src/main.js",
    "test": "jest --max-workers=1",
    "test:coverage": "npm run test --coverage",
    "test:watch": "npm run test -- --watch",
    "typecheck": "npm run build --noemit"
  },
  "dependencies": {
    "aws-sdk": "^2.1313.0",
    "email-validator": "^2.0.4",
    "flatted": "^3.2.7",
    "pino": "^8.10.0",
    "tslib": "~2.4"
  },
  "devDependencies": {
    "@types/jest": "~29.2",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "fast-check": "^3.6.3",
    "husky": "^8.0.3",
    "jest": "29.4",
    "prettier": "~2.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rimraf": "~3.0",
    "sort-package-json": "^2.4.1",
    "ts-jest": "~29.0",
    "tsutils": "~3.21",
    "typescript": "~4.9"
  },
  "engines": {
    "node": ">= 18.12 <19"
  },
  "volta": {
    "node": "18.12.1"
  }
}
